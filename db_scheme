CREATE TABLE BUSES(
	UNIQLINE		INT	PRIMARY KEY NOT NULL,	
	LINE_REF 		INT     NOT NULL,
	DIRECTION_REF           INT    	NOT NULL,
	PUBLISHED_LINE_NAME	INT     NOT NULL,
	OPERATOR_REF		INT	NOT NULL,
	DESTINATION_REF		INT	NOT NULL,
	MONITORING_REF		INT	NOT NULL,
	EXPECTED_ARRIVAL_TIME	INT	NOT NULL,
	STOP_POINT_REF		INT	NOT NULL,
	NODE_START		INT	NOT NULL,
	RESPONSE_KEY		INT	NOT NULL,
	NODE_END		INT	NOT NULL	
);
CREATE TABLE RESPONSES_RAW_DATA(
	RESPONSE_KEY		INT	PRIMARY KEY NOT NULL,	
	RAW_DATA		STRING     NOT NULL,
);